{
  "name": "Supreme",
  "description": "Checkout bot",
  "version": "0.1",
  "manifest_version": 1,
  "permissions": ["*://*/*"],
  "content_scripts": [
    {
      "matches": ["*://*.supremenewyork.com/*"],
      "js": ["jquery.js", "supreme.js"]
    }
  ],
  "browser_action": {
    "default_title": "Enable/Disable"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
}
